{
  "Logging": {
    "IncludeScopes": "false",
    "LogLevel": {
      "Default": "Debug",
      "System": "Error",
      "Microsoft": "Error"
    },
    "File": {
      // 日志文件保存路径
      "Path": "Logs\\{Date}.log",
      "RetainedFileCountLimit": 300,
      "LogLevel": {
        "Default": "warning",
        "System": "warning",
        "Microsoft": "warning"
      }
    }
  }
}
